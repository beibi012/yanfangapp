<view class="problemlist_container"><block wx:for="{{problemlist}}" wx:for-item="problem" wx:for-index="__i0__"><navigator class="search_content" url="{{problem.url}}"><view class="developer">{{problem.developer}}</view><view class="householder">{{problem.householder}}</view><view class="address">{{problem.address}}</view><view class="checkitem">{{problem.main+"-"+problem.checkitem}}</view><view class="problem_content">{{problem.problem_content}}</view><view class="date">{{problem.date}}</view><view class="time">{{problem.time}}</view></navigator></block></view>