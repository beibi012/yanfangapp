
.web_title{
		height: 6vh;
		width: 750rpx;
		margin-bottom: 2%;
		white-space: nowrap;
}
.nav{
		display: inline-block;
		width: 373rpx;
		height: 80rpx;
		line-height: 80rpx;
		text-align: center;
		color: #999;
		border: 1px solid #ddd;
}
.active{
		color: #333;
		border-bottom: #00B4FF solid 5rpx;
}
.show_container{
		width: 750rpx;
		height: 100%;
		overflow: hidden;
}
.details_container{
		position: relative;
		width: 1520rpx;
}
.half{
		display: inline-block;
		width: 750rpx;
		height: 90vh;
		float: left;
}
.picker_container{ 
	height: 100rpx;
	white-space: nowrap;
}
.picker{
		display: inline-block;
		width: 30%;
		float: left;
		border: #ddd 1px solid;
		border-radius: 10rpx;
		margin: 1%;
		color: #00B4FF;
		text-indent: 5%;
}
.arrow{
			position: relative;
			float: right;
			top: -10rpx;
			display: inline-block;
			width: 40rpx;
			height: 40rpx;
			overflow: hidden;
			margin-top: 8%;
			margin-right: 2%;
}
.newly_problem{
	width: 100%;
}
.newly_botton{
		display: inline-block;
		width: 70%;
		margin: 0 15%;
		border: #ddd 1px solid;
		color: #666;
		border-radius: 15rpx;
		text-align: center;
}
.img_container{
	margin: 2%;
}
.img{
		display: block;
		width: 90%;
		margin: 0 5% 0 5%;
}
.problemlist{
	width: 100%;
	height: 60vh;
	overflow: hidden;
}
.problem:first-child{
		border-top: #ddd 1px solid;
}
.problem{
		width: 100%;
		height: 150rpx;
		border-bottom: #ddd 1px solid;
}
.top{
			height: 40rpx;
			white-space: nowrap;
}
.color{
			display: inline-block;
			position: relative;
			left: -520rpx;
			width: 15rpx;
			height: 35rpx;
			margin-top: 1%;
			margin-left: 5%;
}
.color_red{
			background-color: #eb3226;
}
.color_green{
			background-color: #a5d255;
}
.main_item{
			float: left;
			display: inline-block;
			width: 70%;
			color: #333;
			text-indent: 60rpx;
			font-size: 35rpx;
}
.problem_content{
			display: inline-block;
			float: left;
			width: 70%;
			text-indent: 70rpx;
			font-size: 30rpx;
			color: #999;
			white-space: nowrap;
			-o-text-overflow: ellipsis;
			   text-overflow: ellipsis;
			overflow: hidden;
}
.date{
			display: inline-block;
			width: 70%;
			float: left;
			text-indent: 70rpx;
			font-size: 30rpx;
			color: #999;
}
.sate{
			display: inline-block;
			font-size: 35rpx;
			width: 30%;
			height: 140rpx;
			text-align: center;
			line-height: 140rpx;
			position: relative;
			right: 0rpx;
			top:0rpx;
}
.state text{
				margin-left: 5rpx;
}
.dot{
				display: inline-block;
				width: 28rpx;
				height: 28rpx;
				border-radius: 50%;
}
.progress_container{
	position: fixed;
	bottom: 0;
	width: 100%;
	background-color: #fff;
}
.progress{
		width: 90%;
		margin: 0 5%;
}
.text{ 
			font-size: 35rpx;
}
	/* //实测实量 */
input{
			border-radius: 10rpx;
}
.basic_headline{
			color: #00b4ff;
			border-bottom: 0;
			border-top: 0;
			font-size: 40rpx;
}
.content{
			display: inline-block;
			width: 100%;
			padding-bottom: 2%;
			line-height: 50rpx;
			font-size: 30rpx;
			white-space: nowrap;
}
.roomtype{
				margin-left: 2%;
				font-size: 35rpx;
				color: #333;
}
.standard{
				float: right;
				margin-right: 10%;
				color: #333;
}
.right1{
				margin-left: 78%;
				color: #999;
}
.right2{
				margin-left: 3%;
				color: #999;
}
.data_content{
				display: inline-block;
				width: 75%;
}
.wrap{
					white-space: normal;
}
.manyinput{
						display: inline-block;
						width: 70rpx;
						background-color: #fff;
						margin-left: 3%;
						margin-top: 1%;
						border: 1px solid #ddd;
}
.inputcontent{
					width: 100%;
					margin-top: 3%;
					height: 60rpx;
					text-indent: 2%;
}
.inputtitle{
						display: inline-block;
						width: 20%;
						vertical-align: top;
}
.inputtitle text{
							color: #666;
}
.inputvalue{
						display: inline-block;
						width: 75%;
						border: 1px solid #ddd;
}
.standard_container{
			display: inline-block;
				width: 20%;
				height: 355rpx;
				margin-left: 3%;
				margin-top: 2%;
				vertical-align: top;
				border: 1px solid #ddd;
				border-radius: 10rpx;
}
.inputstandard{
				display: inline-block;
				width: 85%;
				margin-left: 2%;
				margin-top: 155rpx;
}
.standard_container2{
			display: inline-block;
			width: 23%;
			margin-left: 3%;
			vertical-align: top;
			white-space: nowrap;
			margin-bottom: 150rpx;
}
.standard_half{
				display: inline-block;
				width: 50%;
				vertical-align: top;
				white-space: normal;
}
.standard_half .manyinput{
					display: inline-block;
					width: 70rpx;
					background-color: #fff;
					margin-top: 6%;
					border: 1px solid #ddd;
}
.submit_container{
					position: fixed;
					bottom: 10rpx;
					background-color: #fff;
					height: 100rpx;
					width: 100%;
}
.submit{
					background-color: #00B4FF;
					width: 75%;
					margin: 0 12.5%;
}


