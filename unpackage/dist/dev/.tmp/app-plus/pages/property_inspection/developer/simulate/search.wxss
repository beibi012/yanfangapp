
.content_container{
	width: 100%;
	height: 90vh;
}
.search_c{
	height: 12vh;
	width: 100%;
	font-size: 35rpx;
	text-indent: 5%;
	border-bottom: #ddd 1px solid;
}
.search_c:first-child{
	border-top: #ddd 1px solid;
}
.search_c .black{
	height: 6vh;
	width: 100%;
	line-height: 8vh;
	color: #333;
	font-weight: 800;
}
.search_c .grey{
	height: 6vh;
	width: 100%;
	line-height: 4vh;
	color: #999;
}
.tip{
	height: 10vh;
	width: 100%;
	line-height: 10vh;
	color: #999;
	text-align: center;
}

